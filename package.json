{
    "name": "dsp",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "ds": "antlr4 -Dlanguage=JavaScript Ds.g4 -visitor -no-listener",
        "dsp": "antlr4 -Dlanguage=JavaScript Dsp.g4 -visitor -no-listener",
        "prepare": "run-p ds dsp",
        "build": "rollup --config rollup.config.js"
    },
    "dependencies": {
        "antlr4": "^4.13.2"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.4",
        "npm-run-all": "^4.1.5",
        "rollup": "^4.52.3"
    }
}
